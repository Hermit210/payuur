{
  "name": "soluma-magicblock",
  "version": "0.1.0",
  "description": "Soluma Event Platform with MagicBlock Ephemeral Rollups",
  "main": "index.js",
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "test": "anchor test",
    "build": "anchor build",
    "deploy": "anchor deploy",
    "deploy:devnet": "anchor deploy --provider.cluster devnet",
    "magicblock:start": "magicblock-engine start",
    "magicblock:test": "anchor test --provider.cluster http://localhost:8899",
    "magicblock:demo": "node scripts/test-magicblock.js",
    "demo": "node scripts/demo.js",
    "setup": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "deploy:full": "bash scripts/deploy-magicblock.sh devnet"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",

    "@solana/web3.js": "^1.95.2"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.0.3",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhruv457457/soLuma.git"
  },
  "author": "Soluma Team",
  "license": "MIT"
}